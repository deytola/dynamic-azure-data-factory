{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AdetolaADF"
		},
		"ADLSg2_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSg2_LS'"
		},
		"GenericAzureSQLDB_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'GenericAzureSQLDB_LS'"
		},
		"SecureAzureSqlDatabase_LS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SecureAzureSqlDatabase_LS'"
		},
		"ADLSg2_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adedp203storageacc.dfs.core.windows.net/"
		},
		"GenericAzureSQLDB_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().serverName}"
		},
		"GenericAzureSQLDB_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().databaseName}"
		},
		"GenericAzureSQLDB_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sysadmin"
		},
		"SecureAzureSqlDatabase_LS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "toladberver.database.windows.net"
		},
		"SecureAzureSqlDatabase_LS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "TolaDB"
		},
		"SecureAzureSqlDatabase_LS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sysadmin"
		},
		"AzureSqlDatabase_MPE_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/8f55ffcf-6dec-4f5e-8357-6e4921d42ef3/resourceGroups/DP203/providers/Microsoft.Sql/servers/toladberver"
		},
		"AzureSqlDatabase_MPE_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AdetolaADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSg2_LS_accountKey": {
						"value": "[parameters('ADLSg2_LS_accountKey')]"
					},
					"GenericAzureSQLDB_LS_password": {
						"value": "[parameters('GenericAzureSQLDB_LS_password')]"
					},
					"SecureAzureSqlDatabase_LS_password": {
						"value": "[parameters('SecureAzureSqlDatabase_LS_password')]"
					},
					"ADLSg2_LS_properties_typeProperties_url": {
						"value": "[parameters('ADLSg2_LS_properties_typeProperties_url')]"
					},
					"GenericAzureSQLDB_LS_properties_typeProperties_server": {
						"value": "[parameters('GenericAzureSQLDB_LS_properties_typeProperties_server')]"
					},
					"GenericAzureSQLDB_LS_properties_typeProperties_database": {
						"value": "[parameters('GenericAzureSQLDB_LS_properties_typeProperties_database')]"
					},
					"GenericAzureSQLDB_LS_properties_typeProperties_userName": {
						"value": "[parameters('GenericAzureSQLDB_LS_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase_MPE_properties_privateLinkResourceId": {
						"value": "[parameters('AzureSqlDatabase_MPE_properties_privateLinkResourceId')]"
					},
					"AzureSqlDatabase_MPE_properties_groupId": {
						"value": "[parameters('AzureSqlDatabase_MPE_properties_groupId')]"
					},
					"SecureAzureSqlDatabase_LS_properties_typeProperties_server": {
						"value": "[parameters('SecureAzureSqlDatabase_LS_properties_typeProperties_server')]"
					},
					"SecureAzureSqlDatabase_LS_properties_typeProperties_database": {
						"value": "[parameters('SecureAzureSqlDatabase_LS_properties_typeProperties_database')]"
					},
					"SecureAzureSqlDatabase_LS_properties_typeProperties_userName": {
						"value": "[parameters('SecureAzureSqlDatabase_LS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}